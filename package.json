{
    "name": "ambient-ts-app",
    "version": "0.1.2",
    "private": true,
    "dependencies": {
        "@crocswap-libs/sdk": "^0.2.13",
        "@emotion/react": "^11.9.0",
        "@emotion/style": "^0.8.0",
        "@emotion/styled": "^11.8.1",
        "@material-ui/core": "^4.12.4",
        "@mui/material": "^5.8.2",
        "@reduxjs/toolkit": "^1.8.1",
        "@rive-app/react-canvas": "^3.0.1",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.2.0",
        "@testing-library/user-event": "^14.1.1",
        "@types/jest": "^27.0.1",
        "@types/node": "^16.7.13",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "@walletconnect/web3-provider": "1.6.6",
        "@web3auth/web3auth": "^0.11.0-alpha.0",
        "ajv": "^8.11.0",
        "ajv-formats": "^2.1.1",
        "ethers": "^5.6.5",
        "framer-motion": "4.1.17",
        "graphql": "^16.5.0",
        "graphql-request": "^4.2.0",
        "jest-canvas-mock": "^2.4.0",
        "magic-sdk": "7.0.0",
        "moralis": "^1.10.0",
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "react-icons": "^4.3.1",
        "react-lottie-player": "^1.4.3",
        "react-moralis": "^1.3.5",
        "react-redux": "^8.0.1",
        "react-router-dom": "^6.3.0",
        "react-scripts": "4.0.3",
        "react-transition-group": "^4.4.2",
        "react-use-websocket": "^4.2.0",
        "rive-react": "^3.0.1",
        "typescript": "^4.4.2",
        "web-vitals": "^2.1.0"
    },
    "scripts": {
        "start": "yarn && react-scripts start",
        "start-only": "react-scripts start",
        "build": "react-scripts --max_old_space_size=3072 build",
        "test": "react-scripts test",
        "test:nowatch": "react-scripts test --watchAll=false",
        "eject": "react-scripts eject",
        "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
        "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
        "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
        "pre-commit": "yarn lint:fix && yarn format && yarn test:nowatch",
        "prepare": "husky install"
    },
    "lint-staged": {
        "**/*.{js,jsx,ts,tsx}": [
            "npx prettier --write --config ./.prettierrc",
            "npx eslint --fix"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@playwright/test": "^1.21.1",
        "@typescript-eslint/eslint-plugin": "^5.22.0",
        "@typescript-eslint/parser": "^5.22.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^2.7.1",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.29.4",
        "eslint-plugin-react-hooks": "^4.5.0",
        "husky": "^8.0.0",
        "lint-staged": "^12.4.1",
        "prettier": "^2.6.2"
    }
}

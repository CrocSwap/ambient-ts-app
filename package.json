{
    "name": "ambient-ts-app",
    "version": "2.0.1",
    "private": true,
    "dependencies": {
        "@covalenthq/client-sdk": "^0.2.6",
        "@crocswap-libs/sdk": "^0.2.80",
        "@d3fc/d3fc-extent": "^4.0.2",
        "@emotion/is-prop-valid": "^1.2.1",
        "@emotion/react": "^11.10.5",
        "@emotion/styled": "^11.10.5",
        "@material-ui/core": "^4.12.4",
        "@mui/material": "^5.8.2",
        "@types/react-color": "^3.0.7",
        "babel-plugin-styled-components": "^2.1.4",
        "d3": "^7.4.4",
        "d3fc": "^15.2.4",
        "emoji-picker-react": "^3.6.2",
        "ethers": "^5.6.5",
        "framer-motion": "4.1.17",
        "i18next": "^21.8.16",
        "i18next-browser-languagedetector": "^6.1.4",
        "intro.js": "^6.0.0",
        "intro.js-react": "^0.7.1",
        "modern-screenshot": "^4.4.25",
        "moment": "^2.29.4",
        "numbro": "^2.3.6",
        "re-resizable": "^6.6.11",
        "react": "^18.1.0",
        "react-color": "^2.19.3",
        "react-dom": "^18.1.0",
        "react-i18next": "^11.18.3",
        "react-icons": "^4.3.1",
        "react-jazzicon": "^1.0.4",
        "react-lottie-player": "^1.4.3",
        "react-reveal": "^1.2.2",
        "react-router-dom": "^6.3.0",
        "react-transition-group": "^4.4.2",
        "react-use-websocket": "^4.2.0",
        "socket.io-client": "^4.5.1",
        "styled-components": "^6.0.3",
        "wagmi": "^0.12.12",
        "web-vitals": "^2.1.0"
    },
    "scripts": {
        "start": "node -v | grep -q \"v16\" && yarn run start:regular || yarn run start:legacy",
        "start:legacy": "yarn && yarn run start-only:legacy",
        "start:regular": "yarn && yarn run start-only:regular",
        "start-only": "node -v | grep -q \"v16\" && yarn run start-only:regular || yarn run start-only:legacy",
        "start-only:legacy": "react-scripts --openssl-legacy-provider start",
        "start-only:regular": "react-scripts start",
        "build": "node -v | grep -q \"v16\" && yarn run build:regular || yarn run build:legacy",
        "build:legacy": "react-scripts --openssl-legacy-provider --max_old_space_size=4096 build",
        "build:regular": "react-scripts --max_old_space_size=4096 build",
        "test": "react-scripts test",
        "test:sequential": "react-scripts test --runInBand --testPathPattern=ambient-utils/tests/sequential",
        "test:parallel": "react-scripts test --testPathPattern=ambient-utils/tests/parallel",
        "test:nowatch": "react-scripts test --watchAll=false",
        "eject": "react-scripts eject",
        "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
        "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
        "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
        "pre-commit": "yarn lint:fix && yarn format && yarn test:nowatch",
        "prepare": "husky install"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "prettier --write --config ./.prettierrc",
            "eslint --fix"
        ]
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not op_mini all"
    ],
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.2.0",
        "@testing-library/user-event": "^14.1.1",
        "@types/adm-zip": "^0.5.4",
        "@types/d3": "^7.1.0",
        "@types/jest": "^29.2.5",
        "@typescript-eslint/eslint-plugin": "^5.22.0",
        "@typescript-eslint/parser": "^5.22.0",
        "adm-zip": "^0.5.10",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^2.7.1",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.29.4",
        "eslint-plugin-react-hooks": "^4.5.0",
        "husky": "^8.0.0",
        "lint-staged": "^12.4.1",
        "playwright": "^1.39.0",
        "prettier": "^2.6.2",
        "react-scripts": "4.0.3",
        "superagent": "^8.1.2",
        "typescript": "^4.4.2"
    },
    "jest": {
        "transformIgnorePatterns": [
            "/node_modules/(?!d3|d3-array|internmap|delaunator|robust-predicates|wagmi|@wagmi|modern-screenshot)"
        ]
    }
}
